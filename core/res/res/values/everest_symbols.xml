<resources>
    <!-- Alert Slider -->
    <java-symbol type="string" name="alert_slider_state_path" />
    <java-symbol type="string" name="alert_slider_uevent_match_path" />
    <java-symbol type="bool" name="config_hasAlertSlider" />

    <!-- Camera -->
    <java-symbol type="array" name="config_cameraAuxPackageAllowList" />
    <java-symbol type="array" name="config_cameraAuxPackageExcludeList" />

    <!-- Cycle through ringer modes -->
    <java-symbol type="string" name="volume_dialog_ringer_guidance_normal" />

    <!-- Define device configs on boot -->
    <java-symbol type="array" name="global_device_configs_override" />
    <java-symbol type="array" name="device_configs_override" />

    <!-- Device keyhandlers -->
    <java-symbol type="array" name="config_deviceKeyHandlerLibs" />
    <java-symbol type="array" name="config_deviceKeyHandlerClasses" />

    <!-- Display color inversion availability -->
    <java-symbol type="bool" name="config_displayInversionAvailable" />

    <!-- Package Manager -->
    <java-symbol type="array" name="config_deviceDisabledComponents" />
    <java-symbol type="array" name="config_globallyDisabledComponents" />
    <java-symbol type="array" name="config_forceEnabledComponents" />

    <!-- Whether to cleanup fingerprints upon connection to the daemon and when user switches -->
    <java-symbol type="bool" name="config_cleanupUnusedFingerprints" />

    <!-- Whether to cancel fingerprint operation if not idle -->
    <java-symbol type="bool" name="config_fpCancelIfNotIdle" />

    <!-- Whether to use Richtap vibration -->
    <java-symbol type="bool" name="config_usesRichtapVibration" />

    <!-- Location indicator -->
    <java-symbol type="drawable" name="stat_sys_location" />

    <!-- Delay in ms before invoking single tap gesture
         Should be set to dt2w max time / 2 when needed -->
    <java-symbol type="integer" name="config_singleTapDelay" />

    <!-- Should we listen for fingerprints when the screen is off?  Devices
         with a rear-mounted sensor want this, but certain devices have
         the sensor embedded in the power key and listening all the time
         causes a poor experience. -->
    <java-symbol type="bool" name="config_fingerprintWakeAndUnlock" />

    <!-- PowerOffAlarmService -->
    <java-symbol type="string" name="notification_channel_poweroff_alarm" />
    <java-symbol type="string" name="poweroff_alarm_title" />
    <java-symbol type="string" name="poweroff_alarm_body" />
    <java-symbol type="drawable" name="ic_alarm_on" />

    <!-- Path to fast charging status file to detect whether an oem fast charger is active -->
    <java-symbol type="string" name="config_oemFastChargerStatusPath" />

    <!-- Expected value from fast charging status file -->
    <java-symbol type="string" name="config_oemFastChargerStatusValue" />

    <!-- App error dialog -->
    <java-symbol type="id" name="aerr_copy" />
    <java-symbol type="string" name="url_copy_success" />
    <java-symbol type="string" name="url_copy_failed" />

    <!-- Whether device supports an alt. ambient display -->
    <java-symbol type="bool" name="config_alt_ambient_display" />

    <!-- SystemUI Restart -->
    <java-symbol type="string" name="systemui_restart_title" />
    <java-symbol type="string" name="systemui_restart_message" />
</resources>
